    {
      "cell_type": "code",
      "execution_count": 117,
      "metadata": {
        "id": "aK_deqs1Izek"
      },
      "outputs": [],
      "source": [
        "def sigmoid_derivative(sigmoid):\n",
        "    return sigmoid * (1 - sigmoid)"
      ]
    },
